@{
  Layout = "_Layout";
}
<body>
  @using VendorAndOrderTracker.Models;
  <h3>All orders by this vendor:</h3>
  <ul>
  @foreach (Order order in @Model["orders"])
  {
    <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.OrderTitle</a></li>
  }
  </ul>
  <p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add new order</a></li>
  <p><a href='/vendors'>Return to vendor list</a></p>
  <p><a href='/'>Return to main page</a></p>
</body>